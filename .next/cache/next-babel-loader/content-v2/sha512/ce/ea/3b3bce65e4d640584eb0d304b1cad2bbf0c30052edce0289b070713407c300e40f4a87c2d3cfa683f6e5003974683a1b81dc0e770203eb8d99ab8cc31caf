{"ast":null,"code":"import React from \"react\";\nvar __jsx = React.createElement;\nimport Navbar from './NavBar';\nimport { useEffect } from 'react';\nimport { useRouter } from 'next/router';\nimport NProgress from 'nprogress';\n\nconst Layout = ({\n  children,\n  title,\n  footer = true,\n  dark = false\n}) => {\n  const router = useRouter();\n  useEffect(() => {\n    const handleRouterChange = url => {\n      NProgress.start();\n    };\n\n    router.events.on('routeChangeStart', handleRouterChange);\n    router.events.on('routeChangeComplete', () => NProgress.done());\n    return () => {\n      router.events.off('routeChangeStart', handleRouterChange);\n    };\n  });\n  return __jsx(\"div\", {\n    className: dark ? 'bg-dark' : ''\n  }, __jsx(Navbar, null), __jsx(\"main\", {\n    className: \"container py-4\"\n  }, title && __jsx(\"h1\", {\n    className: \"text-center text-light\"\n  }, title), children), footer && __jsx(\"footer\", {\n    className: \"bg-dark text-light text-center\"\n  }, __jsx(\"div\", {\n    className: \"container p-4\"\n  }, __jsx(\"h1\", null, \"&copy Ryan Ray Portfolio\"), __jsx(\"p\", null, \" 2000 - \", new Date().getFullYear()), __jsx(\"p\", null, \"All rights reserved.\"))));\n};\n\nexport default Layout;","map":null,"metadata":{},"sourceType":"module"}